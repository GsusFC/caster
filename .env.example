# Database
DATABASE_URL="postgresql://user:password@localhost:5432/farcaster_scheduler?schema=public"

# Neynar API
# Get your API key from https://neynar.com/
NEYNAR_API_KEY="your_neynar_api_key_here"

# Neynar OAuth Client ID
# Get this from your Neynar dashboard for OAuth authentication
NEYNAR_CLIENT_ID="your_neynar_client_id_here"

# NextAuth (generate with: openssl rand -base64 32)
NEXTAUTH_SECRET="your_nextauth_secret_here"
NEXTAUTH_URL="http://localhost:3000"

# Environment
NODE_ENV="development"

# Optional: Analytics, logging, etc.
# SENTRY_DSN=""
# POSTHOG_KEY=""
